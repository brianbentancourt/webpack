{
  "name": "webpack-curso",
  "version": "1.0.0",
  "description": "curso de webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:image": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js",
    "build:react": "webpack --config ./babel-react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js",
    "build:prevent": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:vendor": "webpack --config ./vendor-prevent-duplication/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brianbentancourt/webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Brian Bentancourt",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.4",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "nib": "^1.1.2",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.7",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "rupture": "^0.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "file-loader": "^3.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
